<!DOCTYPE html>
<html>
<body>
<script src="jquery.min.js"></script>
<script src="jquery.cookie.js"></script>
<script>
function gotoPage() {
userid = document.forms["frm1"]["userName"].value;
if (userid == '') {
alert('please enter a username');
return false;
}
$.cookie('transgame_userid', userid, { expires: 7, path: '/' });
window.location = 'lobby.cgi.py';
return false;



}

userid = $.cookie('transgame_userid');
if (userid != null) {
window.location = 'http://gkovacs.xvm.mit.edu:8080/lobby';
}

</script>
<form name="frm1" action="" onsubmit="return gotoPage()">
Your Username: <input type="text" name="userName" />
<input type="submit" value="Enter Username" /><br/>
</body>
</html>
